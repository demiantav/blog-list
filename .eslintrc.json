{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",

  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "import/extensions": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "warn",
    "object-curly-newline": "off",
    "consistent-return": "off"
  }
}
